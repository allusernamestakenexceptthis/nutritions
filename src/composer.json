{
  "name": "gomilkyway/nutritions",
  "description": "A nutritions builder and calculator",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "App/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "vendor-dir": "vendor/"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.1"
  }
}
